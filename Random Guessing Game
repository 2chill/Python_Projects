from random import randint
secret = randint(1,10)

print("The Guessing Game\n")

quess = 0
while secret != guess:
    g = input("Guess a number!: ")
    guess = int(g)
    if guess == secret:
        print("You won!")
    else:
        if guess < secret:
            print("To low!")
        else:
            print("To high!")
            
print("Game Over!!")
